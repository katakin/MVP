<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.second.tabs.third.ThirdTabFragment">

    <TextView
        android:id="@+id/view_id"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        android:gravity="center"
        app:layout_constraintBottom_toTopOf="@+id/presenter_id"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="First Tab View ID = ******" />

    <TextView
        android:id="@+id/presenter_id"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        android:gravity="center"
        app:layout_constraintBottom_toTopOf="@+id/components"
        app:layout_constraintTop_toBottomOf="@+id/view_id"
        tools:text="First Tab Presenter ID = ******" />

    <TextView
        android:id="@+id/components"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="14sp"
        android:gravity="start"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/presenter_id"
        tools:text="Components = ******" />

</android.support.constraint.ConstraintLayout>